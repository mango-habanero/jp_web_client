{
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "insertPragma": false,
    "printWidth": 100,
    "proseWrap": "preserve",
    "quoteProps": "as-needed",
    "rangeStart": 0,
    "requirePragma": false,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 4,
